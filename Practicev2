class Overload:

    def __init__(self, operator):
        self.operator = operator

    def __lt__(self, other):
         if self.operator < other.operator:
             return True
         else:
             return False



x = Overload(16)
y = Overload(15)
print(x == x)
print(x == y)
print(x < y)
print(x > y)

